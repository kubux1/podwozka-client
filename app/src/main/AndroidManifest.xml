<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="podwozka.podwozka">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity"></activity>
        <activity android:name=".RegisterActivity"></activity>
        <activity android:name=".Passenger.PassangerMain"></activity>
        <activity android:name=".Passenger.PassengerFindTravels"></activity>
        <activity android:name=".Passenger.PassengerBrowseTravels"></activity>
        <activity android:name=".Passenger.PassengerTravelsLog"></activity>
        <activity android:name=".Driver.DriverMainActivity"></activity>
        <activity android:name=".Driver.DriverNewTravelActivity"></activity>
        <activity android:name=".Driver.DriverCarInfoActivity"></activity>
        <activity android:name=".Driver.DriverBrowseTravels"></activity>
        <activity android:name=".Driver.DriverTravelEditorActivity"></activity>
        <activity android:name=".Driver.DriverEditTravelInfoActivity"></activity>

    </application>

</manifest>
